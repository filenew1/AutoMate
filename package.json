{
  "name": "agentskills",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "vite dev",
    "electron:build": "tsc && vite build && electron-builder"
  },
  "build": {
    "appId": "com.qqstyle.app",
    "productName": "QQ风格应用",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ]
    }
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-packager": "^17.1.2",
    "electron-winstaller": "^5.4.0",
    "ffi-napi": "^4.0.3",
    "postcss": "^8.5.6",
    "ref-napi": "^3.0.3",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0"
  },
  "dependencies": {
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "daisyui": "^5.5.14",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
